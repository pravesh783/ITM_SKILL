#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int m1,p1,m2,p2;
    cin>>m1>>p1;
    cin>>m2>>p2;
    float v1=p1/m1;
    float v2=p2/m2;
    if(v1>v2){
        cout<<"Total Minimum Cost="<<v2*n<<endl;
    }
    else{
        cout<<"Total Minimum Cost="<<v1*n<<endl;
    }

    return 0;
}
